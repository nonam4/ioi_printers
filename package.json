    "electron": "^9.0.4",
    "electron-builder": "^22.7.0",
    "electron-reload": "^1.5.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "electron-download-manager": "^2.1.2",
    "electron-updater": "^4.3.1",
    "my-local-ip": "^1.0.0",
    "net-snmp": "^2.6.7"
  },
  "build": {
    "appId": "ioiprinters",
    "linux": {
      "icon": "resources/icon.png",
      "target": [
        "deb"
